{
  "name": "macdal-slack-plugin",
  "version": "0.0.1",
  "description": "AI powered slack plugin for software devleopment",
  "main": "app.js",
  "scripts": {
    "start": "tsc && node -r dotenv/config ./dist/app.js",
    "build": "tsc",
    "dev": "nodemon --watch src --ext ts --exec 'tsc && node -r dotenv/config ./dist/app.js'",
    "ngrok": "ngrok http --domain=meerkat-valid-skylark.ngrok-free.app 3000",
    "lint": "eslint . --ext .ts"
  },
  "author": "Tryfifty Inc",
  "license": "MIT",
  "keywords": [
    "slack",
    "bolt",
    "slackapi",
    "typescript",
    "macdal",
    "langchain",
    "openai",
    "qdrant",
    "notion",
    "supabase"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/slack-samples/bolt-ts-starter-template.git"
  },
  "bugs": {
    "url": "https://github.com/slack-samples/bolt-ts-starter-template/issues"
  },
  "dependencies": {
    "@langchain/community": "^0.2.17",
    "@langchain/core": "^0.2.15",
    "@langchain/openai": "^0.2.1",
    "@langchain/qdrant": "^0.0.5",
    "@langchain/textsplitters": "^0.0.3",
    "@notionhq/client": "^2.2.15",
    "@qdrant/js-client-rest": "^1.10.0",
    "@slack/bolt": "^3.19.0",
    "@supabase/supabase-js": "^2.44.3",
    "dotenv": "~16.4.5",
    "express": "^4.19.2",
    "faiss-node": "^0.5.1",
    "langchain": "^0.2.8",
    "langsmith": "^0.1.37",
    "notion-to-md": "^3.1.1",
    "openai": "^4.54.0",
    "pickleparser": "^0.2.1",
    "slackify-markdown": "^4.4.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@typescript-eslint/eslint-plugin": "^8.1.0",
    "@typescript-eslint/parser": "^8.1.0",
    "eslint": "^9.9.0",
    "eslint-config-airbnb-base": "~15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
